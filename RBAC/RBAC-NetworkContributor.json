{
    "Name":  "NetworkContributor",
    "ID":"",
    "IsCustom":  true,
    "Description":  "Lets you manage internal networks, but not create network components that connect to the Internet.",
    "Actions":  [
                    "Microsoft.Authorization/*/read",
                    "Microsoft.Insights/alertRules/*",
                    "Microsoft.Network/*",
                    "Microsoft.ResourceHealth/availabilityStatuses/read",
